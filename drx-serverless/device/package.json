{
  "name": "drx-serverless-device",
  "version": "1.1.0",
  "description": "DRX Serverless - device service",
  "main": "src/handler.js",
  "scripts": {
    "serverless:offline:dev:pc": "$env:SLS_DEBUG='*'; $env:ENVIRONMENT='dev'; $env:AWS_REGION='ap-southeast-2'; $env:PROJECT_CODE='drx'; $env:PROVIDER='device'; $env:DEPLOY_S3_BUCKET='cochlear-drx-cx-dev-deployment'; $env:CI_PIPELINE_IID='CI_PIPELINE_IID'; npx serverless offline start --config serverless_offline.yml",
    "serverless:offline:sit:pc": "$env:SLS_DEBUG='*'; $env:ENVIRONMENT='sit'; $env:AWS_REGION='ap-southeast-2'; $env:PROJECT_CODE='drx'; $env:PROVIDER='device'; $env:DEPLOY_S3_BUCKET='cochlear-drx-cx-dev-deployment'; $env:CI_PIPELINE_IID='CI_PIPELINE_IID'; npx serverless offline start --config serverless_offline.yml",
    "serverless:offline:dev:mac": "SLS_DEBUG=* ENVIRONMENT=dev AWS_REGION=ap-southeast-2 PROJECT_CODE=drx PROVIDER=device DEPLOY_S3_BUCKET=cochlear-drx-cx-dev-deployment CI_PIPELINE_IID=CI_PIPELINE_IID npx serverless offline start --config serverless_offline.yml",
    "serverless:offline:sit:mac": "SLS_DEBUG=* ENVIRONMENT=sit AWS_REGION=ap-southeast-2 PROJECT_CODE=drx PROVIDER=device DEPLOY_S3_BUCKET=cochlear-drx-cx-dev-deployment CI_PIPELINE_IID=CI_PIPELINE_IID npx serverless offline start --config serverless_offline.yml",
    "format-check": "npx prettier --check ./src/**/*",
    "format": "npx prettier --write ./src/**/*",
    "lint": "npx eslint src test",
    "lint-fix": "npx eslint --fix src test",
    "test": "mocha test/unit/**/*.test.js --max-http-header-size=80000 --reporter mocha-junit-reporter --reporter-options outputs=true",
    "test-inline": "mocha test/unit/**/*.test.js",
    "coverage": "nyc --reporter html --reporter lcov mocha test/unit/**/*.test.js",
    "open-report": "open coverage/index.html"
  },
  "author": "Soundwave",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@gitlab.cochlear.link:drx/drx-serverless.git"
  },
  "dependencies": {
    "axios": "^1.7.5",
    "hashmap": "^2.4.0",
    "implement-js": "0.0.31",
    "ioredis": "^5.4.1",
    "jsforce": "^3.4.1",
    "lodash": "^4.17.21",
    "node-json-transform": "^1.1.2",
    "type-detect": "^4.0.8",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.572.0",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@eslint/js": "^9.2.0",
    "api-gateway-stage-tag-plugin": "^1.0.6",
    "chai": "^4.3.4",
    "dotenv": "^16.4.5",
    "eslint": "9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "mocha": "^10.4.0",
    "mocha-junit-reporter": "^2.2.1",
    "nock": "^13.5.4",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "proxyquire": "^2.1.3",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.5.0",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-plugin-optimize": "^4.1.0-rc.1",
    "serverless-plugin-warmup": "^8.3.0",
    "serverless-prune-plugin": "^2.0.2",
    "sinon": "^17.0.2"
  }
}
