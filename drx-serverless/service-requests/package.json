{
  "name": "drx-serverless-service-requests",
  "version": "1.0.0",
  "description": "Drx Serverless - service requests service",
  "main": "src/handler.js",
  "scripts": {
    "serverless:offline:dev:pc": "$env:SLS_DEBUG='*'; $env:ENVIRONMENT='dev'; $env:AWS_REGION='ap-southeast-2'; $env:PROJECT_CODE='drx'; $env:PROVIDER='service-requests'; $env:DEPLOY_S3_BUCKET='cochlear-drx-cx-dev-deployment'; $env:CI_PIPELINE_IID='CI_PIPELINE_IID'; npx serverless offline start --config serverless_offline.yml",
    "serverless:offline:sit:pc": "$env:SLS_DEBUG='*'; $env:ENVIRONMENT='sit'; $env:AWS_REGION='ap-southeast-2'; $env:PROJECT_CODE='drx'; $env:PROVIDER='service-requests'; $env:DEPLOY_S3_BUCKET='cochlear-drx-cx-dev-deployment'; $env:CI_PIPELINE_IID='CI_PIPELINE_IID'; npx serverless offline start --config serverless_offline.yml",
    "serverless:offline:dev:mac": "SLS_DEBUG=* ENVIRONMENT=dev AWS_REGION=ap-southeast-2 PROJECT_CODE=drx PROVIDER=service-requests DEPLOY_S3_BUCKET=cochlear-drx-cx-dev-deployment CI_PIPELINE_IID=CI_PIPELINE_IID npx serverless offline start --config serverless_offline.yml",
    "serverless:offline:sit:mac": "SLS_DEBUG=* ENVIRONMENT=sit AWS_REGION=ap-southeast-2 PROJECT_CODE=drx PROVIDER=service-requests DEPLOY_S3_BUCKET=cochlear-drx-cx-dev-deployment CI_PIPELINE_IID=CI_PIPELINE_IID npx serverless offline start --config serverless_offline.yml",
    "format-check": "npx prettier --check ./src/**/*",
    "format": "npx prettier --write ./src/**/*",
    "lint": "npx eslint src test",
    "lint-fix": "npx eslint --fix src test",
    "test": "mocha test/unit/**/*.test.js --max-http-header-size=80000 --reporter mocha-junit-reporter --reporter-options outputs=true",
    "test-inline": "mocha test/unit/**/*.test.js",
    "coverage": "nyc --reporter html --reporter lcov mocha test/unit/**/*.test.js",
    "open-report": "open coverage/index.html",
    "integration-test": "mocha --timeout 10000 --max-http-header-size=80000 test/integration/**/*.test.js"
  },
  "author": "Soundwave",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@gitlab.cochlear.link:drx/drx-serverless.git"
  },
  "dependencies": {
    "axios": "^1.7.5",
    "ioredis": "^5.4.1",
    "jsforce": "^3.4.1",
    "lodash": "^4.17.21",
    "simple-crypto-js": "^3.0.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.572.0",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@eslint/js": "^9.2.0",
    "chai": "^4.3.4",
    "eslint": "9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "mocha": "^10.4.0",
    "mocha-junit-reporter": "^2.2.1",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "serverless-offline": "^13.5.0",
    "sinon": "^17.0.2"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Mocha Test Explorer",
      "properties": {
        "mochaExplorer.timeout": {
          "description": "The test-case timeout in milliseconds",
          "type": "number",
          "default": 50000,
          "scope": "resource"
        }
      }
    }
  }
}
