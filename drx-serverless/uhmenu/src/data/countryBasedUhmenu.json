      "content": [
        {
          "image": {
            "href": "81437f55-ff49-4f09-9f1e-8ceb1eab5ad4/whatsnew_personal_details%402x.png?MOD=AJPERES",
            "alt-text": "personal details"
          },
          "title": "New look personal details",
          "text": "We have a new and simpler way for you to view and edit your personal details. Just select About me from your profile menu and check your details.",
