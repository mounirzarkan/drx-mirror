{
  "name": "node-headless-ssr-proxy-sample",
  "version": "13.0.0",
  "description": "Node server-side-rendering proxy sample for running JSS apps under Node hosting",
  "main": "dist/index.js",
  "scripts": {
    "start": "node index.js",
    "update-newRelic-GroupName-non-prod": "depid='DRX Non Prod' && sed -i -e 's/CochlearDRXNewRelic/'\"$depid\"'/g' ./newrelic.js",
    "update-newRelic-GroupName-prod": "depid='DRX Prod' && sed -i -e 's/CochlearDRXNewRelic/'\"$depid\"'/g' ./newrelic.js"
  },
  "engines": {
    "node": ">=8.1",
    "npm": ">=5.6.0",
    "yarn": "yarn is not supported, please use npm"
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "homepage": "https://jss.sitecore.net",
  "license": "Apache-2.0",
  "dependencies": {
    "@sitecore-jss/sitecore-jss-proxy": "^13.0.0",
    "append-query": "^2.1.0",
    "compression": "~1.7.3",
    "cors": "^2.8.5",
    "express": "~4.16.4",
    "ipaddr.js": "~1.8.1",
    "newrelic": "^6.2.0",
    "node-cache": "~4.2.0",
    "node-fetch": "~2.3.0"
  },
